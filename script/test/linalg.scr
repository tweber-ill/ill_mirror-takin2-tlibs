main()
{
	vec1 = [1, 2, 3];
	vec2 = vec1 / len(vec1);

	print("length: " + len(vec1));
	print("norm length: " + len(vec2));


	mat1 = [[1,2], [3,4]];
	print("matrix: " + str(mat1));
	print("det: " + det(mat1));
	print("transpose: " + str(trans(mat1)));
	print("inverse: " + str(inv(mat1)));
	print("invinv: " + str(inv(inv(mat1))));

	print("unit matrix: " + str(unity(4)));
	print("3x4 matrix: " + str(mat(3,4)));

	print("sum: " + str(unity(4) + unity(4)));

	print("prod: " + str(prod([[0,1],[1,0]], [1,0])));
	print("prod: " + str(prod([0,1], [[0,1],[1,0]])));
	print("prod: " + str(prod([[0,1],[1,0]], [[0,1],[1,0]])));

	mat1 = [[1,2],[3,4],[5,6]];
	mat2 = [[1,2,3],[4,5,6]];
	print("prod: " + str(prod(mat2,mat1)));



	print(det(inv([[1.2,2.3],[3.4,4.5]])));
	print(1./det([[1.2,2.3],[3.4,4.5]]));
}
